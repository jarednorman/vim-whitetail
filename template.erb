hi clear

if exists("syntax_on")
  syntax reset
endif

let colors_name = "whitetail"

<% theme.each_syntax_element do |name, guibg, guifg, gui| %>
hi <%= name %> guibg=<%= guibg %> guifg=<%= guifg %> gui=<%= gui %>
<% end %>
